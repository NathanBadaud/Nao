        self.bMustStop = False;
        self.bIsRunning = False;

    def onLoad(self):
        self.bMustStop = False;
        self.bIsRunning = False;

    def onUnload(self):
        self.onInput_onStop(); # stop current loop execution

    def onInput_onStart(self):
        self.log( self.boxName + ": start - begin" );
        
        if( self.bIsRunning ):
            self.log( self.boxName + ": already started => nothing" );
            return;

        self.bIsRunning = True;
        self.bMustStop = False;
        
        rTime = self.getParameter( 'rTime' );
        while( not self.bMustStop ):
            for i in range( 8 ):
                ALLeds.fadeRGB( 'FaceLed%d' % i, 0xFFFFFF, rTime );
                ALLeds.post.fadeRGB( 'FaceLed%d' % i, 0x000000, rTime*3 );
        # end while
        self.bIsRunning = False;
        self.onStopped();    
        self.log( self.boxName + ": start - end" ); 

    def onInput_onStop(self):
        self.bMustStop = True; # stop current loop execution]]>
