                                                                                                <![CDATA[import time

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.asr = ALProxy("ALSpeechRecognition")

    def onLoad(self):
        #~ puts code for box initialization here
        self.bInitializing = False
        self.bMustStop = False

    def onUnload(self):
        if( self.bInitializing ):
            self.bMustStop = True
        while( self.bInitializing ):
            time.sleep( 0.2 )

    def onInput_onStart(self):
        if(self.bInitializing):
            return
        self.bInitializing = True
        self.bMustStop = False
        try:
            if( not self.bMustStop ):
                self.asr.setVisualExpression(self.getParameter("Visual expression"))
            if( not self.bMustStop ):
                self.asr.setWordListAsVocabulary( self.getParameter("Word list").split(';') )
            if( not self.bMustStop ):
                self.onReady()
            else:
                self.onStopped()
        except RuntimeError, e:
            self.log("Error occured in SpeechRecognition: " + str(e))
            self.bInitializing = False
            raise e
        self.bInitializing = False
        
    def onInput_onStop(self):
        self.onUnload()]]>
