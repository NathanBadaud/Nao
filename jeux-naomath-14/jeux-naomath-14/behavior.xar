                                                                            <![CDATA[import random

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        
        self.tts = ALProxy('ALTextToSpeech')
        self.ttsStop = ALProxy('ALTextToSpeech', True) 
        self.storage = ALProxy("ALMemory")
        self.multiList =[ 2, 3, 4, 5, 6, 7, 8, 9, 10]  
    
    def onLoad(self):
       
        
        self.aShuffled = []
        self.tab = int(ALMemory.getData("table"))
        
        pass

    def onUnload(self):
        #~ puts code for box cleanup here
        pass

    def onInput_onStart(self):
        
        #verifie liste vide
        list = len(self.multiList)
        if (list==0):
            self.multiList =[ 2, 3, 4, 5, 6, 7, 8, 9, 10]
        else:    
            pass
            
        #generate randone number and erase
        
        nRND=random.choice(self.multiList)
        self.multiList.remove(nRND)
  
       #make the multiplication A VOIR
   
        multi=nRND*self.tab
        
        #save number
        loc=self.getParameter("Location")
        self.storage.insertData(loc, multi)
       
    
        #Ask Question
        tts = ALProxy( "ALTextToSpeech" )
    
        if( tts.getLanguage() == 'French' ): 
            qst = "\RSPD=" + str( self.getParameter("Vocal Speed") ) + "\ "
            qst += "\VCT=" + str( self.getParameter("Vocal Shaping") ) + "\ "
            qst += "Combien font"  + " "
            qst +=  str(self.tab) + " fois " + str( nRND)
            qst += "\RST\ "
            self.tts.say(qst)
            self.onStopped() #~ active la boite de sortie
        
        else:  
            qst = "\RSPD=" + str( self.getParameter("Vocal Speed") ) + "\ "
            qst += "\VCT=" + str( self.getParameter("Vocal Shaping") ) + "\ "
            qst += "how many" + " "
            qst +=  str( self.tab) + " times " + str( nRND)
            qst += "\RST\ "
            self.tts.say(qst) 
            self.onStopped() #~ activate output of the box
         
        pass

    def onInput_onStop(self):
        self.onUnload() #~ it is recommanded to call onUnload of this box in a onStop method, as the code written in onUnload is used to stop the box as well
        pass]]>
