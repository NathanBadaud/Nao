        GeneratedClass.__init__(self)
        self.bMustStop = False
        self.bIsRunning = False
        self.color = 0
        self.blink = 0        
        self.colorPresets = {"WHITE" : 0xFF44CC, "RED" : 0xFF0000, "GREEN" : 0x00FF00, "BLUE" : 0x0000FF, "YELLOW" : 0xFF5511, "PURPLE" : 0xFF00FF, "CYAN" : 0xFFFFFF}

    def onLoad(self):
        #~ puts code for box initialization here
        pass

    def onUnload(self):
        self.onInput_onStop();

    def onInput_onStart(self):
        if(self.bIsRunning): return        

        self.bMustStop = False;
        self.bIsRunning = True;
        
        while(not self.bMustStop):
            self.doBlink()
            if(self.bMustStop): break
            period = self.getParameter("BlinkMean")
            var = self.getParameter("BlinkVariance")
            period += random.uniform(-var, var)
            time.sleep(period)
            
        self.bIsRunning = False;
        self.onStopped();
        
    def onInput_onStop(self):
        self.bMustStop = True
        
    def setupEyeColor(self):
        pColor = self.getParameter("EyeColor")
        try:
            self.color = self.colorPresets[pColor]       
        except KeyError:
            self.color = int("0x" + pColor, 16)
  
    def setupBlinkColor(self):
        color = self.color
        shade = random.uniform(0.11, 0.33) # make each blink shade vary slightly for more realism
        # seperate the base color into its channels
        blue =  (color & 255) / float(255) 
        green = ((color >> 8) & 255) / float(255)
        red = ((color >> 16) & 255) / float(255)
        # convert color space
        hsvBase = colorsys.rgb_to_hsv(red, green, blue)
        # calc shade        
        hsvShade = (hsvBase[0], hsvBase[1], hsvBase[2] * shade)
        # convert back        
        rgbShade = colorsys.hsv_to_rgb(hsvShade[0], hsvShade[1], hsvShade[2])  
        self.blink = (int(rgbShade[0] * 255) << 16) + (int(rgbShade[1] * 255) << 8) + int(rgbShade[2] * 255)
    
    def doBlink(self):
        self.setupEyeColor()
        if(self.bMustStop): return
        
        self.setupBlinkColor()   
        if(self.bMustStop): return
            
        speed = random.uniform(0.075, 0.25) # make the speed of each blink vary slightly for more realism
        ALLeds.post.fadeRGB("FaceLed0", 0x000000, speed * .33)
